@Library('mylibrary')_

pipeline
{
    agent any
    stages
    {
        stage('ContDownload_loans')
        {
            steps
            {
                script
                {
                    cicd.newDownload("maven.git")
                }
            }
        }
        stage('ContBuild_loans')
        {
            steps
            {
                script
                {
                    cicd.newBuild()
                }
            }
        }
    }
}
